{
  "name": "@logue/vue2-helpers",
  "version": "2.0.2",
  "description": "A util package to use Vue 2 with Composition API easily",
  "keywords": [
    "vue2",
    "vue2.7",
    "utils",
    "helpers",
    "composition",
    "vuex",
    "vue-router",
    "vuetify"
  ],
  "author": {
    "name": "Ambit Tsai",
    "email": "ambit_tsai@qq.com"
  },
  "contributors": [
    {
      "name": "Logue",
      "url": "https://logue.dev/",
      "email": "logue@hotmail.co.jp"
    }
  ],
  "license": "Apache-2.0",
  "main": "index.umd.js",
  "module": "index.es.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.es.js",
      "require": "./index.umd.js",
      "types": "./index.d.ts"
    },
    "./vuex": {
      "import": "./vuex.es.js",
      "require": "./vuex.umd.js",
      "types": "./vuex.d.ts"
    },
    "./vue-router": {
      "import": "./vue-router.es.js",
      "require": "./vue-router.umd.js",
      "types": "./vue-router.d.ts"
    },
    "./vuetify": {
      "import": "./vuetify.es.js",
      "require": "./vuetify.umd.js",
      "types": "./vuetify.d.ts"
    }
  },
  "files": [
    "./*.d.ts",
    "./*.d.ts.map",
    "./*.es.js",
    "./*.es.js.map",
    "./*.umd.js",
    "./*.umd.js.map"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=16.17.0",
    "yarn": ">=1.22.10"
  },
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "build": "node scripts/build.js && tsc --declaration --emitDeclarationOnly",
    "build:clean": "node scripts/clean.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logue/vue2-helpers.git"
  },
  "homepage": "https://github.com/logue/vue2-helpers#readme",
  "bugs": {
    "url": "https://github.com/logue/vue2-helpers/issues"
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.7.0",
    "vue": "^2.0.0 || <3.0.0",
    "vue-router": "^3.6.4",
    "vuetify": "^2.6.9",
    "vuex": "^3.6.2"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    },
    "vuetify": {
      "optional": true
    },
    "vuex": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.4.0",
    "@types/eslint": "^8.4.6",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.23.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-jsonc": "^2.4.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.8.2",
    "vue": "^2.7.10",
    "vue-router": "^3.6.4",
    "vue-tsc": "^0.40.5",
    "vuetify": "^2.6.9",
    "vuex": "^3.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "prettier": "^2.7.1"
  }
}
